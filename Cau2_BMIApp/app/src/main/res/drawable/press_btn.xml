<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <size android:width="100dp"
                android:height="60dp"/>
            <gradient android:startColor="#909FE6"
                android:endColor="#B58BDA"/>
            <corners android:radius="30dp"/>
            <stroke android:color="#B8B4B4"
                android:width="1dp"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <size android:width="100dp"
                android:height="60dp"/>
            <gradient android:startColor="#1830A8"
                android:endColor="#C95BDC"/>
            <corners android:radius="30dp"/>
            <stroke android:color="#B8B4B4"
                android:width="1dp"/>
        </shape>
    </item>
</selector>